                              .form-group.col-md-4
                                 label.control-label(for='module') 所属模块

                                 input#module.form-control(type='text' name='module')

                              .form-group.col-md-4
                                 label.control-label(for='simpleDesc') 简要描述

                                 input#simpleDesc.form-control(type='text' name='simpleDesc')

                              .form-group.col-md-4
                                 label.control-label(for='priority ') 优先级

                                 select.selectpicker.form-control#priority(name='priority')
                                    option(value = '01') 一般
                                    option(value = '02') 重大
                                    option(value = '03') 紧急

                              .form-group.col-md-4
                                 label.control-label(for='startTime') 发现时间

                                 input#startTime.form-control(type='text' name='startTime')

                              .form-group.col-md-4
                                 label.control-label(for='dealTime') 预计处理时限

                                 input#dealTime.form-control(type='text' name='dealTime')

                              .form-group.col-md-4
                                 label.control-label(for='mark') 备注

                                 input#mark.form-control(type='text' name='mark')

                              .form-group
                                 label(for='description') 详细描述

                                 textarea#description.form-control(type='text' rows='3' name='description')

                              .form-group
                                 button.btn.btn-primary.btn-sm(type='button' onclick='saveQm()') 保存记录




