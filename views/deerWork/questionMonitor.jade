                        .box-content

                           table.table.table-striped.table-bordered.bootstrap-datatable.datatable.responsive
                              thead
                                 tr
                                    th 所属模块
                                    th 简要描述
                                    th 优先级
                                    th 状态
                                    th 预计处理时限
                                    th 操作

                              tbody#questionMonitorList
                                 -for (var i = 0; i < questionMonitors.length; i++)
                                    tr
                                       td=questionMonitors[i].module
                                       td=questionMonitors[i].simpleDesc
                                       td=questionMonitors[i].priority
                                       td
                                          span#statusShow.label-success.label.label-default=questionMonitors[i].status
                                          |&nbsp
                                          select#StatusSelect
                                             option(value = 'Issue') Issue
                                             option(value = 'Open') Open
                                             option(value = 'Fixed') Fixed
                                             option(value = 'Closed') Closed
                                             option(value = 'Published') Published
                                             option(value = 'NotProblem') NotProblem
                                       td=questionMonitors[i].dealTime
                                       td
                                          a.btn.btn-success(href='javascript:void(0);' onclick='viewDetail("'+questionMonitors[i].id+'")')
                                             i.glyphicon.glyphicon-zoom-in.icon-white
                                             |  查看
                                          |&nbsp

                                          a.btn.btn-info(href='javascript:void(0);' onclick='updateStatus("'+questionMonitors[i].id+'",this)')
                                             i.glyphicon.glyphicon-edit.icon-white
                                             |  状态保存
                                          |&nbsp

                                          a.btn.btn-danger(href='javascript:void(0);' onclick ='deleteQm("'+questionMonitors[i].id+'")')
                                             i.glyphicon.glyphicon-trash.icon-white
                                             |  删除
